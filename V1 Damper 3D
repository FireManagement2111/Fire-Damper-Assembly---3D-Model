    <div id="footer">
        <h3>Component Key:</h3>
        <ul>
            <li><strong>1.</strong> Duct (Left)</li>
            <li><strong>2.</strong> Duct (Right)</li>
            <li><strong>3.</strong> Fire Damper Housing</li>
            <li><strong>4.</strong> Intumescent Fire Damper Blades</li>
            <li><strong>5.</strong> Wall Penetration</li>
            <li><strong>6.</strong> Angle Flange Connection</li>
            <li><strong>7.</strong> Sealant</li>
            <li><strong>8.</strong> Aluminium Sleeve Connection</li>
            <li><strong>9.</strong> Aluminium Rivets</li>
            <li><strong>10.</strong> Nylon Bolts (Ø8 mm max)</li>
            <li><strong>11.</strong> Access Panel</li>
        </ul>
    </div>

    <div id="imageModal">
        <button id="closeModal">×</button>
        <img id="modalImage" src="" alt="Expanded view">
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js"></script>
    <script>
        // Define image modal functions FIRST - before anything else
        function openImageModal(src, alt) {
            const imageModal = document.getElementById('imageModal');
            const modalImage = document.getElementById('modalImage');
            modalImage.src = src;
            modalImage.alt = alt;
            imageModal.classList.add('show');
        }

        function showComponentImage(componentNumber) {
            const placeholderSrc = 'data:image/svg+xml,' + encodeURIComponent(
                '<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">' +
                '<rect width="800" height="600" fill="#f5f5f5"/>' +
                '<rect x="50" y="50" width="700" height="500" fill="white" stroke="#ddd" stroke-width="2"/>' +
                '<text x="400" y="280" font-family="Arial" font-size="28" font-weight="bold" fill="#2c3e50" text-anchor="middle">' +
                'Component ' + componentNumber + ': Angle Flange Connection' +
                '</text>' +
                '<text x="400" y="340" font-family="Arial" font-size="18" fill="#999" text-anchor="middle">' +
                'Image placeholder - Add your image here' +
                '</text>' +
                '</svg>'
            );
            openImageModal(placeholderSrc, 'Component ' + componentNumber + ' - Angle Flange Connection');
        }

        // THREE.js scene setup
        const scene = new THREE.Scene();
        scene.background = new THREE.Color(0xf0f0f0);
        
        const container = document.getElementById('container');
        const camera = new THREE.PerspectiveCamera(50, container.clientWidth / container.clientHeight, 0.1, 1000);
        camera.position.set(8, 6, 10);
        camera.lookAt(0, 0, 0);

        const renderer = new THREE.WebGLRenderer({ antialias: true });
        renderer.setSize(container.clientWidth, container.clientHeight);
        renderer.shadowMap.enabled = true;
        container.appendChild(renderer.domElement);

        const ambientLight = new THREE.AmbientLight(0xffffff, 0.6);
        scene.add(ambientLight);
        
        const directionalLight = new THREE.DirectionalLight(0xffffff, 0.8);
        directionalLight.position.set(5, 10, 5);
        directionalLight.castShadow = true;
        scene.add(directionalLight);

        const directionalLight2 = new THREE.DirectionalLight(0xffffff, 0.4);
        directionalLight2.position.set(-5, 5, -5);
        scene.add(directionalLight2);

        const ductMaterial = new THREE.MeshStandardMaterial({ color: 0xc0c0c0, metalness: 0.6, roughness: 0.4 });
        const wallMaterial = new THREE.MeshStandardMaterial({ color: 0xe0e0e0, roughness: 0.8 });
        const damperMaterial = new THREE.MeshStandardMaterial({ color: 0x909090, metalness: 0.5, roughness: 0.5 });
        const sealantMaterial = new THREE.MeshStandardMaterial({ color: 0x404040, roughness: 0.9 });

        const leftDuct = new THREE.Mesh(new THREE.BoxGeometry(3, 2, 2), ductMaterial);
        leftDuct.position.set(-3, 0, 0);
        scene.add(leftDuct);

        const rightDuct = new THREE.Mesh(new THREE.BoxGeometry(3, 2, 2), ductMaterial);
        rightDuct.position.set(3, 0, 0);
        scene.add(rightDuct);

        const wall = new THREE.Mesh(new THREE.BoxGeometry(0.3, 4, 3), wallMaterial);
        wall.position.set(0, 0, 0);
        scene.add(wall);

        const housing = new THREE.Mesh(new THREE.BoxGeometry(0.8, 2.2, 2.2), damperMaterial);
        housing.position.set(0, 0, 0);
        scene.add(housing);

        const bladeCount = 7;
        const bladeSpacing = 2 / (bladeCount + 1);
        for (let i = 0; i < bladeCount; i++) {
            const blade = new THREE.Mesh(
                new THREE.BoxGeometry(0.6, 0.08, 1.8), 
                new THREE.MeshStandardMaterial({ color: 0xffffff, roughness: 0.7, opacity: 0.9, transparent: true })
            );
            blade.position.set(0, -1 + bladeSpacing * (i + 1), 0);
            scene.add(blade);
        }

        const flangeMaterial = new THREE.MeshStandardMaterial({ color: 0x808080, metalness: 0.8, roughness: 0.3 });
        
        // Function to create L-shaped angle with proper dimensions
        function createAngleFlange(length) {
            const group = new THREE.Group();
            
            // Vertical leg of the L-angle (thinner and taller)
            const vertical = new THREE.Mesh(
                new THREE.BoxGeometry(0.05, 0.25, length),
                flangeMaterial
            );
            vertical.position.set(0, 0.125, 0);
            
            // Horizontal leg of the L-angle (thinner and wider)
            const horizontal = new THREE.Mesh(
                new THREE.BoxGeometry(0.25, 0.05, length),
                flangeMaterial
            );
            horizontal.position.set(0.125, 0, 0);
            
            group.add(vertical);
            group.add(horizontal);
            return group;
        }
        
        // Top flange (runs along top edge)
        const topFlange = createAngleFlange(2);
        topFlange.rotation.z = Math.PI / 2;
        topFlange.position.set(-1.5, 1.125, 0);
        scene.add(topFlange);
        
        // Bottom flange (runs along bottom edge)
        const bottomFlange = createAngleFlange(2);
        bottomFlange.rotation.z = -Math.PI / 2;
        bottomFlange.position.set(-1.5, -1.125, 0);
        scene.add(bottomFlange);
        
        // Left flange (runs along left edge)
        const leftFlange = createAngleFlange(2);
        leftFlange.rotation.x = Math.PI / 2;
        leftFlange.position.set(-1.5, 0, -1.125);
        scene.add(leftFlange);
        
        // Right flange (runs along right edge)
        const rightFlange = createAngleFlange(2);
        rightFlange.rotation.x = -Math.PI / 2;
        rightFlange.position.set(-1.5, 0, 1.125);
        scene.add(rightFlange);

        const sealantGeometry = new THREE.CylinderGeometry(0.1, 0.1, 2, 16);
        const topSealant = new THREE.Mesh(sealantGeometry, sealantMaterial);
        topSealant.rotation.x = Math.PI / 2;
        topSealant.position.set(0.2, 1.15, 0);
        scene.add(topSealant);

        const bottomSealant = new THREE.Mesh(sealantGeometry, sealantMaterial);
        bottomSealant.rotation.x = Math.PI / 2;
        bottomSealant.position.set(0.2, -1.15, 0);
        scene.add(bottomSealant);

        const sleeveGeometry = new THREE.BoxGeometry(0.5, 0.1, 2.1);
        const sleeveMaterial = new THREE.MeshStandardMaterial({ color: 0xb0b0b0, metalness: 0.8, roughness: 0.3 });
        
        const topSleeve = new THREE.Mesh(sleeveGeometry, sleeveMaterial);
        topSleeve.position.set(2.5, 1, 0);
        scene.add(topSleeve);

        const bottomSleeve = new THREE.Mesh(sleeveGeometry, sleeveMaterial);
        bottomSleeve.position.set(2.5, -1, 0);
        scene.add(bottomSleeve);

        const rivetGeometry = new THREE.CylinderGeometry(0.04, 0.04, 0.15, 8);
        const rivetMaterial = new THREE.MeshStandardMaterial({ color: 0xa0a0a0, metalness: 0.9, roughness: 0.2 });
        const rivetPositions = [
            [2.5, 1, -0.8], [2.5, 1, -0.4], [2.5, 1, 0], [2.5, 1, 0.4], [2.5, 1, 0.8],
            [2.5, -1, -0.8], [2.5, -1, -0.4], [2.5, -1, 0], [2.5, -1, 0.4], [2.5, -1, 0.8]
        ];
        
        rivetPositions.forEach(pos => {
            const rivet = new THREE.Mesh(rivetGeometry, rivetMaterial);
            rivet.position.set(pos[0], pos[1], pos[2]);
            rivet.rotation.z = Math.PI / 2;
            scene.add(rivet);
        });

        const accessPanel = new THREE.Mesh(
            new THREE.CylinderGeometry(0.4, 0.4, 0.05, 32), 
            new THREE.MeshStandardMaterial({ color: 0xa0a0a0, metalness: 0.3, roughness: 0.6 })
        );
        accessPanel.rotation.z = Math.PI / 2;
        accessPanel.position.set(3.5, -0.8, 0);
        scene.add(accessPanel);

        const boltGeometry = new THREE.CylinderGeometry(0.04, 0.04, 0.2, 8);
        const boltMaterial = new THREE.MeshStandardMaterial({ color: 0x303030, metalness: 0.6, roughness: 0.4 });
        
        // Top bolts
        const boltPositionsTop = [
            [-1.5, 1.15, -0.8], [-1.5, 1.15, -0.4], [-1.5, 1.15, 0.4], [-1.5, 1.15, 0.8]
        ];
        boltPositionsTop.forEach(pos => {
            const bolt = new THREE.Mesh(boltGeometry, boltMaterial);
            bolt.position.set(pos[0], pos[1], pos[2]);
            bolt.rotation.x = Math.PI / 2;
            scene.add(bolt);
        });
        
        // Bottom bolts
        const boltPositionsBottom = [
            [-1.5, -1.15, -0.8], [-1.5, -1.15, -0.4], [-1.5, -1.15, 0.4], [-1.5, -1.15, 0.8]
        ];
        boltPositionsBottom.forEach(pos => {
            const bolt = new THREE.Mesh(boltGeometry, boltMaterial);
            bolt.position.set(pos[0], pos[1], pos[2]);
            bolt.rotation.x = Math.PI / 2;
            scene.add(bolt);
        });
        
        // Left side bolts
        const boltPositionsLeft = [
            [-1.5, 0.8, -1.15], [-1.5, 0.4, -1.15], [-1.5, -0.4, -1.15], [-1.5, -0.8, -1.15]
        ];
        boltPositionsLeft.forEach(pos => {
            const bolt = new THREE.Mesh(boltGeometry, boltMaterial);
            bolt.position.set(pos[0], pos[1], pos[2]);
            bolt.rotation.z = Math.PI / 2;
            scene.add(bolt);
        });
        
        // Right side bolts
        const boltPositionsRight = [
            [-1.5, 0.8, 1.15], [-1.5, 0.4, 1.15], [-1.5, -0.4, 1.15], [-1.5, -0.8, 1.15]
        ];
        boltPositionsRight.forEach(pos => {
            const bolt = new THREE.Mesh(boltGeometry, boltMaterial);
            bolt.position.set(pos[0], pos[1], pos[2]);
            bolt.rotation.z = Math.PI / 2;
            scene.add(bolt);
        });

        function createNumberMarker(number, position) {
            const canvas = document.createElement('canvas');
            const context = canvas.getContext('2d');
            canvas.width = 128;
            canvas.height = 128;
            
            context.fillStyle = 'rgba(255, 200, 0, 0.95)';
            context.beginPath();
            context.arc(64, 64, 50, 0, Math.PI * 2);
            context.fill();
            
            context.strokeStyle = 'rgba(0, 0, 0, 0.8)';
            context.lineWidth = 4;
            context.beginPath();
            context.arc(64, 64, 50, 0, Math.PI * 2);
            context.stroke();
            
            context.fillStyle = 'black';
            context.font = 'Bold 48px Arial';
            context.textAlign = 'center';
            context.textBaseline = 'middle';
            context.fillText(number, 64, 64);
            
            const texture = new THREE.Texture(canvas);
            texture.needsUpdate = true;
            const sprite = new THREE.Sprite(new THREE.SpriteMaterial({ map: texture }));
            sprite.position.set(position[0], position[1], position[2]);
            sprite.scale.set(0.6, 0.6, 1);
            sprite.userData.componentNumber = number; // Store the component number
            return sprite;
        }

        const marker1 = createNumberMarker('1', [-3, 2.2, 0]);
        scene.add(marker1);
        const marker2 = createNumberMarker('2', [3, 2.2, 0]);
        scene.add(marker2);
        const marker3 = createNumberMarker('3', [0, 2.2, 0]);
        scene.add(marker3);
        const marker4 = createNumberMarker('4', [0.6, 0, 0]);
        scene.add(marker4);
        const marker5 = createNumberMarker('5', [0, -2.5, 0]);
        scene.add(marker5);
        const marker6 = createNumberMarker('6', [-1.8, 1.5, 0]);
        scene.add(marker6);
        const marker7 = createNumberMarker('7', [0.6, 1.5, 0]);
        scene.add(marker7);
        const marker8 = createNumberMarker('8', [2.8, 1.4, 0]);
        scene.add(marker8);
        const marker9 = createNumberMarker('9', [2.8, 0.6, 0]);
        scene.add(marker9);
        const marker10 = createNumberMarker('10', [-1.8, 0.7, 0]);
        scene.add(marker10);
        const marker11 = createNumberMarker('11', [4.2, -0.8, 0]);
        scene.add(marker11);

        // Store all markers for raycasting
        const markers = [marker1, marker2, marker3, marker4, marker5, marker6, marker7, marker8, marker9, marker10, marker11];

        let isDragging = false;
        let previousMousePosition = { x: 0, y: 0 };
        let rotation = { x: 0.3, y: 0.5 };
        let radius = 12;

        // Raycaster for detecting clicks on markers
        const raycaster = new THREE.Raycaster();
        const mouse = new THREE.Vector2();

        renderer.domElement.addEventListener('mousedown', (e) => {
            isDragging = true;
            previousMousePosition = { x: e.clientX, y: e.clientY };
        });

        // Change cursor on hover
        renderer.domElement.addEventListener('mousemove', (e) => {
            if (isDragging) {
                const deltaX = e.clientX - previousMousePosition.x;
                const deltaY = e.clientY - previousMousePosition.y;
                rotation.y += deltaX * 0.005;
                rotation.x += deltaY * 0.005;
                rotation.x = Math.max(-Math.PI / 2, Math.min(Math.PI / 2, rotation.x));
                previousMousePosition = { x: e.clientX, y: e.clientY };
            } else {
                // Check if hovering over marker 6
                const rect = renderer.domElement.getBoundingClientRect();
                mouse.x = ((e.clientX - rect.left) / rect.width) * 2 - 1;
                mouse.y = -((e.clientY - rect.top) / rect.height) * 2 + 1;
                
                raycaster.setFromCamera(mouse, camera);
                const intersects = raycaster.intersectObjects(markers);
                
                if (intersects.length > 0 && intersects[0].object.userData.componentNumber === '6') {
                    renderer.domElement.style.cursor = 'pointer';
                } else {
                    renderer.domElement.style.cursor = isDragging ? 'grabbing' : 'grab';
                }
            }
        });

        renderer.domElement.addEventListener('mouseup', (e) => {
            // Check if this was a click (not a drag)
            const dragDistance = Math.sqrt(
                Math.pow(e.clientX - previousMousePosition.x, 2) + 
                Math.pow(e.clientY - previousMousePosition.y, 2)
            );
            
            if (dragDistance < 5) { // Consider it a click if moved less than 5 pixels
                // Calculate mouse position in normalized device coordinates
                const rect = renderer.domElement.getBoundingClientRect();
                mouse.x = ((e.clientX - rect.left) / rect.width) * 2 - 1;
                mouse.y = -((e.clientY - rect.top) / rect.height) * 2 + 1;
                
                // Update the raycaster
                raycaster.setFromCamera(mouse, camera);
                
                // Check for intersections with markers
                const intersects = raycaster.intersectObjects(markers);
                
                if (intersects.length > 0) {
                    const clickedMarker = intersects[0].object;
                    const componentNum = clickedMarker.userData.componentNumber;
                    
                    // Only show modal for marker 6
                    if (componentNum === '6') {
                        showComponentImage(6);
                    }
                }
            }
            
            isDragging = false;
        });
        
        renderer.domElement.addEventListener('mouseleave', () => isDragging = false);

        renderer.domElement.addEventListener('wheel', (e) => {
            e.preventDefault();
            radius = Math.max(5, Math.min(25, radius - e.deltaY * 0.002));
        }, { passive: false });

        function animate() {
            requestAnimationFrame(animate);
            camera.position.x = Math.cos(rotation.y) * Math.cos(rotation.x) * radius;
            camera.position.y = Math.sin(rotation.x) * radius;
            camera.position.z = Math.sin(rotation.y) * Math.cos(rotation.x) * radius;
            camera.lookAt(0, 0, 0);
            renderer.render(scene, camera);
        }
        animate();

        window.addEventListener('resize', () => {
            camera.aspect = container.clientWidth / container.clientHeight;
            camera.updateProjectionMatrix();
            renderer.setSize(container.clientWidth, container.clientHeight);
        });

        function toggleCategory(element) {
            element.classList.toggle('open');
            const content = element.nextElementSibling;
            content.classList.toggle('show');
        }

        function toggleDamperType(element) {
            element.classList.toggle('open');
            const content = element.nextElementSibling;
            content.classList.toggle('show');
        }

        function toggleSubitem(element) {
            element.classList.toggle('open');
            const content = element.nextElementSibling;
            content.classList.toggle('show');
        }

        function toggleType(element) {
            element.classList.toggle('open');
            const content = element.nextElementSibling;
            content.classList.toggle('show');
            
            // Add click handlers to images when content is expanded
            setTimeout(() => {
                const images = content.querySelectorAll('img');
                images.forEach(img => {
                    img.onclick = function() {
                        openImageModal(this.src, this.alt);
                    };
                });
            }, 100);
        }

        // Image modal functionality
        const imageModal = document.getElementById('imageModal');
        const closeModalBtn = document.getElementById('closeModal');

        // Close modal when clicking the X button
        closeModalBtn.onclick = function() {
            imageModal.classList.remove('show');
        };

        // Close modal when clicking outside the image
        imageModal.onclick = function(e) {
            if (e.target === imageModal) {
                imageModal.classList.remove('show');
            }
        };

        // Close modal with Escape key
        document.onkeydown = function(e) {
            if (e.key === 'Escape' && imageModal.classList.contains('show')) {
                imageModal.classList.remove('show');
            }
        };
    </script>
</body>
</html>
